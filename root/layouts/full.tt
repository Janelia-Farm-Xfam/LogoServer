<!doctype html>
<!--[if lt IE 7]> <html class="no-js ie6 oldie" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js ie7 oldie" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js ie8 oldie" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

	<title>[% c.stash.title || 'LogoServer' %]</title>
	<meta name="description" content="">
	<meta name="author" content="">

	<meta name="viewport" content="width=device-width,initial-scale=1">
  <link rel="stylesheet" href="/static/css/main.css">

	<script src="/static/js/libs/modernizr-2.0.6.min.js"></script>
</head>
<body>

  <div id="container">
    <header role="banner" class="ie_clearfix">
      <h1>
        LogoServer
      </h1>
    </header>

    <div id="message" [% IF c.stash.error || c.stash.message -%]class="warning"[% END -%]>
      [% FOR error IN c.stash.error -%]
        [% IF error.safe %]
          [% FOR message IN error.value %]
            <p id="[% error.id %]">[% message %]</p>
          [% END %]
        [% ELSE %]
          [% FOR message IN error.value %]
            <p id="[% error.id %]" >[% message | html %]</p>
          [% END %]
        [% END -%]
      [% END -%]
      [% IF c.stash.message %]
        <p>[% c.stash.message %]</p>
      [% END %]
    </div>

    <div id="content" class="clearfix">
      [%- content -%]
    </div>

    <footer>
    </footer>


  </div> <!--! end of #container -->

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="/static/js/libs/jquery-1.7.1.min.js"><\/script>')</script>

<script src="/static/js/scroller.js"></script>
<script src="/static/js/hmm_logo.js"></script>
<script src="/static/js/application.js"></script>

</body>
</html>
